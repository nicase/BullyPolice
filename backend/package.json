    "commander": "^2.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "emailjs": "^2.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.2.0",
    "handlebars": "^4.7.7",
    "http-status": "^1.3.2",
    "mailer": "^0.6.7",
    "method-override": "^3.0.0",
    "mongoose": "^5.6.6",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
