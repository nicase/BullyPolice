{
  "name": "nkm-pushserver",
  "version": "1.1.0",
  "description": " Push Notification microservice",
  "keywords": [
    "push",
    "apn",
    "firebase",
    "mongodb"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/nkm_solutions/nkm-pushserver"
  },
  "scripts": {
    "start": "node ./src/httpServer.js",
    "start:dev": "node ./src/httpServer.js",
